<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:local="clr-namespace:BiliPlayer.Resource">
	<Style
		x:Key="FontAwesome">
		<Setter
			Property="FontFamily"
			Value="pack://application:,,,/Resource/#FontAwesome" />
		<Setter
			Property="FontSize"
			Value="14" />
		<Setter
			Property="VerticalAlignment"
			Value="Center" />
		<Setter
			Property="Foreground"
			Value="{DynamicResource themeActiveColor}" />
	</Style>
</ResourceDictionary>
